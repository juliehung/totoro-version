{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": { "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }] },
  "parser": "babel-eslint",
  "plugins": ["babel", "import", "jsx-a11y", "react", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
